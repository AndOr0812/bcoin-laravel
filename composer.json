    "keywords": ["tpenaranda", "bcoin", "laravel", "btc", "api", "node"],
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Tate Pe√±aranda",
            "email": "tate@blockinit.io"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~7",
        "laravel/framework": "^6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "TPenaranda\\BCoin\\": "src/"
        }
